import java.util.*;
class RemoveDupChar
{
     public static void main(String []args)
     {
      Scanner rv = new Scanner(System.in);
      System.out.print("Enter the string : ");
      String a = rv.nextLine();
 
      char ch;
      String finalString= "";
      
      for(int i=0; i<a.length(); i++)
      {
          ch = a.charAt(i);
   
        int found = 0;
        for(int j=0; j<finalString.length(); j++)
        {
         if(ch == finalString.charAt(j))
         {
          found=1;
          break;
         }
        }

        if(found == 0)
        { 
         finalString = finalString + ch;
        }
       }
       System.out.print("After removing string is : " + finalString);
     }
}
        
      
