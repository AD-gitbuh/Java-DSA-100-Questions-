import java.util.*;
class WhiteSpaceRemove
{
     public static void main(String []args)
     {
      Scanner rv = new Scanner(System.in);
      System.out.print("Enter the string : ");
      String a = rv.nextLine();
      
      char ch;

      for(int i = 0; i < a.length(); i++)
      {
       ch = a.charAt(i);
       if(a.charAt(i) >= 'a' && a.charAt(i) <= 'z')
       {
        System.out.print(ch);
       }
      }
     }
}
      
